	sleep 10000;
	aimings = 0;
}

restore_db()
{
	sleep 2000;
	aimings=0;
}

AimPrimary(h, p)
{
	signal 4;
	set-signal-mask 4;
	aimings=1;
	start-script smoke();
	turn hip to y-axis h speed <500>;
	sleep (Rand( 1, 5000 ));
	start-script restore_db();
	return (1);
}

SweetSpot(piecenum)
{
	piecenum = sweet-spot;
}

Killed(severity, corpsetype)
{
	if( severity <= 50 )
	{
		corpsetype = 1;
		return (0);
	}
	corpsetype = rand(2,6);
	return (0);
}
